{
    "name": "multiuser-editor",
    "private": true,
    "workspaces": [
        "apps/*",
        "packages/*"
    ],
    "scripts": {
        "dev:server": "npm run dev -w apps/server",
        "dev:web": "npm run dev -w apps/web",
        "dev": "concurrently \"npm run dev:server\" \"npm run dev:web\""
    },
    "devDependencies": {
        "concurrently": "^8.2.2"
    }
}